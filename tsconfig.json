{
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "outDir": "./dist/",
    "module": "ESNext",
    "esModuleInterop": true,
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "jsxImportSource": "@emotion/react",
    "baseUrl": "src",
    "paths": {
      "@core/*": ["core/*"],
      "@constants/*": ["constants/*"],
      "@di/*": ["frameworks/di/*"],
      "@components/*": ["frameworks/components/*"],
      "@services/*": ["frameworks/services/*"],
      "@hooks/*": ["frameworks/hooks/*"]
    }
  }
}
